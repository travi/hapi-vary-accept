{
  "name": "hapi-vary-accept",
  "description": "hapi pre-response handler to include Accept in the Vary header",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/hapi-vary-accept",
  "bugs": "https://github.com/travi/hapi-vary-accept/issues",
  "homepage": "https://npm.im/hapi-vary-accept",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:peer": "npm ls >/dev/null",
    "lint:md": "remark . --frail",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "generate:md": "remark . --output",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.25.9",
    "@hapi/hapi": "21.3.12",
    "@travi/any": "3.1.2",
    "@travi/babel-preset": "3.0.135",
    "@travi/eslint-config": "5.0.11",
    "@travi/eslint-config-mocha": "1.0.25",
    "c8": "10.1.2",
    "chai": "4.5.0",
    "commitlint-config-travi": "1.4.51",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "mocha": "10.8.2",
    "npm-run-all2": "7.0.1",
    "publint": "0.2.12",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.6",
    "remark-toc": "9.0.0",
    "rimraf": "6.0.1",
    "rollup": "4.27.2",
    "sinon": "19.0.2"
  },
  "peerDependencies": {
    "@hapi/hapi": "^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0"
  }
}
