{
  "name": "hapi-vary-accept",
  "description": "hapi pre-response handler to include Accept in the Vary header",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "repository": "travi/hapi-vary-accept",
  "bugs": "https://github.com/travi/hapi-vary-accept/issues",
  "homepage": "https://npm.im/hapi-vary-accept",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "test": "run-s lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "^1.7.5",
    "@travi/eslint-config-travi": "^1.6.14",
    "babel-preset-env": "^1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "codecov": "^3.0.2",
    "commitlint-config-travi": "1.2.31",
    "cz-conventional-changelog": "2.1.0",
    "doctoc": "1.4.0",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "^1.0.0-rc.8",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "rollup": "^0.67.3",
    "sinon": "^6.0.0",
    "travis-lint": "^1.0.0"
  },
  "peerDependencies": {
    "hapi": "^17.0.0"
  }
}
